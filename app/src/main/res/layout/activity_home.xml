<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:openDrawer="start"
    android:id="@+id/drawer"
    tools:context=".activities.Home">
    <RelativeLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/rl_top_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/tool_bar"
                android:layout_width="@dimen/dp_35"
                android:layout_height="@dimen/dp_35"
                android:visibility="gone" />
            <ImageView
                android:layout_marginVertical="@dimen/dp_10"
                android:layout_marginHorizontal="@dimen/dp_15"
                android:id="@+id/bt_toolbar"
                android:src="@mipmap/ic_menu"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"/>
            <TextView
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:textSize="@dimen/sp_22"
                android:textColor="@color/color_primary"
                android:fontFamily="@font/baloo2_bold"
                android:id="@+id/tv_title"/>
            <ImageView
                android:id="@+id/iv_home"
                android:layout_centerHorizontal="true"
                android:layout_width="@dimen/dp_125"
                android:layout_height="@dimen/dp_50"
                android:src="@drawable/img_app_logo"/>
            <TextView
                android:layout_marginVertical="@dimen/dp_10"
                android:layout_marginHorizontal="@dimen/dp_15"
                android:text="@string/report"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_primary"
                android:fontFamily="@font/baloo2_semibold"
                android:id="@+id/bt_report"/>
        </RelativeLayout>
        <FrameLayout
            android:id="@+id/fl_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/rl_top_bar"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_menu"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@color/white"
        android:clickable="true">
        <ImageView
            android:id="@+id/iv_logo"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_centerHorizontal="true"
            android:layout_width="@dimen/dp_200"
            android:layout_height="@dimen/dp_55"
            android:src="@drawable/img_app_logo"/>
        <androidx.core.widget.NestedScrollView
            android:layout_marginTop="@dimen/dp_20"
            android:layout_below="@id/iv_logo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:id="@+id/mn_home"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_home"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/home"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_pf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_pf"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/my_profile"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_10"
                    android:id="@+id/mn_search"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_40"
                    android:background="@drawable/bg_et_search"
                    android:layout_marginTop="@dimen/dp_20">
                    <ImageView
                        android:layout_marginStart="@dimen/dp_10"
                        android:src="@mipmap/ic_search"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/search"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_notifi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_bell"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/notification"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_mixer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_mixers"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/mixers"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_mess"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_msg"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/messages"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_likes"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_likes"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/likes"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_maybe"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_maybe"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/maybe"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_liked"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_liked"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/liked"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_match"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@drawable/ic_mn_match"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/matchmaking"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_partner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@drawable/ic_mn_partner"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/partner_apps"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_qa"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_qa"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/q_amp_a"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_delete_acc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_delete_acc"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/delete_account"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_term"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_alert"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/terms"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_contact"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_letter"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/contact_us"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_logout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        android:src="@mipmap/ic_logout"
                        android:layout_width="@dimen/dp_24"
                        android:layout_height="@dimen/dp_24"/>
                    <TextView
                        android:text="@string/logout"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/baloo2_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/gray3"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <View
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:background="@color/gray5"/>
                <TextView
                    android:text="@string/patent_pending"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_15"
                    android:fontFamily="@font/baloo2_medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/tv_app_des"
                    android:layout_marginBottom="@dimen/dp_20"
                    android:layout_marginStart="@dimen/dp_20"
                    android:fontFamily="@font/baloo2_medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/gray3"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.core.widget.NestedScrollView>
    </RelativeLayout>
</androidx.drawerlayout.widget.DrawerLayout>