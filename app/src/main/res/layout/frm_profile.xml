<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.FrmProfile"
    android:background="@color/white"
    android:paddingHorizontal="@dimen/dp_15">
    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/ll_content"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <ImageView
            android:layout_gravity="center_horizontal"
            android:layout_width="@dimen/dp_150"
            android:layout_height="@dimen/dp_55"
            android:src="@drawable/img_app_logo"/>
        <androidx.cardview.widget.CardView
            app:cardBackgroundColor="@android:color/transparent"
            android:foreground="@drawable/bg_img_outline"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_gravity="center_horizontal"
            android:layout_width="@dimen/dp_120"
            android:layout_height="@dimen/dp_120"
            app:cardCornerRadius="@dimen/dp_60"
            app:cardElevation="0dp">
            <ImageView
                android:src="@drawable/no_avatar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/iv_user"/>
        </androidx.cardview.widget.CardView>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:visibility="gone"
            android:layout_marginTop="@dimen/dp_5"
            android:id="@+id/ll_2_img"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/dp_4"
                android:layout_height="@dimen/dp_4"
                app:cardBackgroundColor="@color/black"
                app:cardElevation="0dp"
                app:cardCornerRadius="@dimen/dp_3"/>
            <androidx.cardview.widget.CardView
                android:layout_marginStart="@dimen/dp_5"
                android:layout_width="@dimen/dp_4"
                android:layout_height="@dimen/dp_4"
                app:cardBackgroundColor="@color/black"
                app:cardElevation="0dp"
                app:cardCornerRadius="@dimen/dp_3"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:visibility="gone"
            android:layout_marginTop="@dimen/dp_5"
            android:id="@+id/ll_3_img"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/dp_4"
                android:layout_height="@dimen/dp_4"
                app:cardBackgroundColor="@color/black"
                app:cardElevation="0dp"
                app:cardCornerRadius="@dimen/dp_3"/>
            <androidx.cardview.widget.CardView
                android:layout_marginStart="@dimen/dp_5"
                android:layout_width="@dimen/dp_4"
                android:layout_height="@dimen/dp_4"
                app:cardBackgroundColor="@color/black"
                app:cardElevation="0dp"
                app:cardCornerRadius="@dimen/dp_3"/>
            <androidx.cardview.widget.CardView
                android:layout_marginStart="@dimen/dp_5"
                android:layout_width="@dimen/dp_4"
                android:layout_height="@dimen/dp_4"
                app:cardBackgroundColor="@color/black"
                app:cardElevation="0dp"
                app:cardCornerRadius="@dimen/dp_3"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <TextView
            android:layout_marginTop="@dimen/dp_15"
            android:id="@+id/tv_name"
            android:textSize="@dimen/sp_22"
            android:fontFamily="@font/baloo2_bold"
            android:gravity="center_horizontal"
            android:singleLine="true"
            android:ellipsize="end"
            android:textColor="@color/color_primary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <TextView
            android:layout_marginTop="@dimen/dp_25"
            android:id="@+id/tv_religion"
            android:textSize="@dimen/sp_18"
            android:fontFamily="@font/baloo2_medium"
            android:textColor="@color/color_primary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <View
            android:layout_marginVertical="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/gray6"/>
        <TextView
            android:id="@+id/tv_stt"
            android:textSize="@dimen/sp_18"
            android:fontFamily="@font/baloo2_medium"
            android:textColor="@color/color_primary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <View
            android:layout_marginVertical="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/gray6"/>
        <TextView
            android:id="@+id/tv_goal"
            android:textSize="@dimen/sp_18"
            android:fontFamily="@font/baloo2_medium"
            android:textColor="@color/color_primary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <View
            android:layout_marginVertical="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/gray6"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/tv_height"
                android:textSize="@dimen/sp_18"
                android:fontFamily="@font/baloo2_medium"
                android:textColor="@color/color_primary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_alignParentEnd="true"
                android:id="@+id/tv_weight"
                android:textSize="@dimen/sp_18"
                android:fontFamily="@font/baloo2_medium"
                android:textColor="@color/color_primary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>
        <View
            android:layout_marginVertical="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/gray6"/>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/ll_phone">
            <TextView
                android:id="@+id/tv_phone"
                android:textSize="@dimen/sp_18"
                android:fontFamily="@font/baloo2_medium"
                android:textColor="@color/color_primary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <View
                android:layout_marginVertical="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/gray6"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <TextView
            android:id="@+id/tv_state"
            android:textSize="@dimen/sp_18"
            android:fontFamily="@font/baloo2_medium"
            android:textColor="@color/color_primary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <View
            android:layout_marginVertical="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/gray6"/>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/ll_origin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tv_origin"
                android:textSize="@dimen/sp_18"
                android:fontFamily="@font/baloo2_medium"
                android:textColor="@color/color_primary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <View
                android:layout_marginVertical="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/gray6"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <TextView
            android:id="@+id/tv_profession"
            android:textSize="@dimen/sp_18"
            android:fontFamily="@font/baloo2_medium"
            android:textColor="@color/color_primary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <View
            android:layout_marginVertical="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/gray6"/>
        <TextView
            android:textSize="@dimen/sp_18"
            android:fontFamily="@font/baloo2_semibold"
            android:text="@string/comments"
            android:textColor="@color/color_primary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv_bio"
            android:textSize="@dimen/sp_18"
            android:fontFamily="@font/baloo2_medium"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <com.google.android.material.button.MaterialButton
            android:stateListAnimator="@null"
            android:layout_marginBottom="@dimen/dp_15"
            android:id="@+id/bt_edit_pf"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:backgroundTint="@color/white"
            app:strokeWidth="@dimen/dp_1"
            app:strokeColor="@color/color_primary"
            app:cornerRadius="@dimen/dp_20"
            android:textColor="@color/color_accent"
            android:text="@string/edit_profile"
            android:textAllCaps="false"
            android:textSize="@dimen/sp_16"
            android:fontFamily="@font/baloo2_semibold"
            android:layout_marginTop="@dimen/dp_30"/>
    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.core.widget.NestedScrollView>